{
    "name": "marcogermani87/filament-hcatpcha",
    "description": "A package to easily include HCaptcha in Filament",
    "keywords": [
        "marcogermani87",
        "laravel",
        "filament",
        "filament-hcatpcha"
    ],
    "homepage": "https://github.com/marcogermani87/filament-hcatpcha",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Germani",
            "email": "info@marcogermani.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0|^11.0",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "MarcoGermani87\\FilamentHcaptcha\\": "src/",
            "MarcoGermani87\\FilamentHcaptcha\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcoGermani87\\FilamentHcaptcha\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcoGermani87\\FilamentHcaptcha\\FilamentHcaptchaServiceProvider"
            ],
            "aliases": {
                "FilamentHcaptcha": "MarcoGermani87\\FilamentHcaptcha\\Facades\\FilamentHcaptcha"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
